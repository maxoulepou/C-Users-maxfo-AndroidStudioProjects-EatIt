<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="75dp"
    android:background="@drawable/fond_liste">

    <TextView
        android:id="@+id/tv_poids"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="4dp"
        android:text="Poids"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_valeurpoids"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_date" />

    <TextView
        android:id="@+id/tv_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:layout_weight="1"
        android:background="@color/blue"
        android:text="59/57/2222"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_valeurpoids"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_weight="1"
        android:text="20"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/tv_poids"
        app:layout_constraintTop_toBottomOf="@id/tv_poids" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="5dp"
        android:text="kg"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_valeurpoids"
        app:layout_constraintTop_toBottomOf="@+id/tv_poids" />

    <TextView
        android:id="@+id/tv_imc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="24dp"
        android:text="IMC"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_poids"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_valeurimc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="26"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_poids"
        app:layout_constraintTop_toBottomOf="@+id/tv_imc" />

    <TextView
        android:id="@+id/tv_tt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="24dp"
        android:text="TT"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_imc"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_graisse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="24dp"
        android:text="Graisse"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_tt"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_muscle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="24dp"
        android:text="Muscle"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_graisse"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_valeurtt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="90"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_imc"
        app:layout_constraintTop_toBottomOf="@+id/tv_tt" />


    <TextView
        android:id="@+id/tv_valeurg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="50"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_tt"
        app:layout_constraintTop_toBottomOf="@+id/tv_graisse" />

    <TextView
        android:id="@+id/tv_valeurm"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="20"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tv_graisse"
        app:layout_constraintTop_toBottomOf="@+id/tv_muscle" />

</androidx.constraintlayout.widget.ConstraintLayout>